# Notion API Integration Token
# Get this from: https://www.notion.so/my-integrations
NOTION_TOKEN=your_notion_token_here

# Default Notion Block/Page ID
# This is the ID of your default Notion page
DEFAULT_BLOCK_ID=your_default_block_id_here

# n8n Webhook URL
# Your n8n workflow webhook endpoint
N8N_WEBHOOK_URL=your_webhook_url_here

# Server Port (optional, defaults to 8000)
PORT=8000
